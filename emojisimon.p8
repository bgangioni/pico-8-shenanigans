pico-8 cartridge // http://www.pico-8.com
version 43
__lua__
function _init()
  
  vel= 10
  xtri= 64
  ytri= 64
  
end

function _update()

  --xtri= xtri-1
  --ytri= ytri+1
  if btn(⬆️) then
  ytri= ytri-vel
  
  end
  
  if btn(⬇️) then
  ytri= ytri+vel
  
  end
  
  if btn(➡️) then
  xtri= xtri+vel
  
  end
  
  if btn(⬅️) then
  xtri= xtri-vel
  end

end

function _draw()

  cls()
  --pset(64,64,10)
  spr(1,xtri-8,ytri-8)
  spr(2,xtri,ytri-8)
  spr(17,xtri-8,ytri)
  spr(18,xtri,ytri)
  --spr(4,xtri,ytri)
  
  
end


