  print(oscilator)
  for i = -1, 1, 0.01 do
    dx,dy = normalize(i,i)
    pset(dx+j,dy,i*100+(counter2*oscilator))
--    pset(dx+(j+1)*oscilator,dy,i*100+counter2)
--    pset(dx+(j+1)*oscilator,dy+oscilator,i*100+counter2)
  end  
  end

end


