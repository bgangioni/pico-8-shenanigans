function _init()
  x = 0
  y = 0

  dx = nil
  dy = nil

  counter = 0
  oscilator = 1
end

function normalize(x,y)
  xx = (x * 64) + 64
  yy = (y * 64 * (-1)) + 64
  return xx, yy
end

function _update()
  counter = counter + 1
  counter2 =counter
end

function _draw()

  cls()
 for j = -128,128, 16 do
