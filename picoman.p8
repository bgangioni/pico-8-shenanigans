version 43
__lua__
function normalize()
end

function _init()
		poke(0x5f2e,1)
		pal(4,131,1)

	px, py = 64,64
end

function _update()
		if btn(⬆️) then py-=1 end
		if btn(⬇️) then py+=1 end
		if btn(⬅️) then px-=1 end
		if btn(➡️) then px+=1 end
end

function _draw()
		cls()
		rectfill(28,32,38,42,4)
		rectfill(38,32,48,42,3)
		print('picoman', 28, 32, 3)
		spr(2,px,py)
end
