function _init()
 palt(0,false)
 cont=0
 vel=10
 troll=0
 camin=false
end

function _update()
 if btn(➡️)then
  
  camin=true
  cont+=1
  if cont > vel then
   cont=0
   troll+=1
    sfx(1) 
   if troll > 1 then
   troll=0
   end
  end
 else
  camin=false
 end
end

function _draw()
 cls(15)
 if camin then
  spr(32+troll*4,4,112,4,2)
 else
  spr(1,12,112,2,2)
 end
end 
 

