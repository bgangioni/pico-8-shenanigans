version 43
__lua__
function _init()
  cont=0
  vel= 3
  --xtri= 64
  ytri= 64
  ani=0
end

function _update()

  --xtri= xtri-1
  --ytri= ytri+1
  if btn(⬆️) then
  ytri= ytri-vel
  
  end
  
  if btn(⬇️) then
  ytri= ytri+vel
  
  end
  
--  if btn(➡️) then
 -- xtri= xtri+vel
  
 -- end
  
  --if btn(⬅️) then
 -- xtri= xtri-vel
 -- end
 if cont==7 then
   ani = ani +1
   if ani==5 then ani=0 end
   cont=0
 end
 cont=cont+1
end

function _draw()

  cls()
  --pset(64,64,10)
  spr(1,4,ytri-8,2,2)
  --spr(4,xtri,ytri)
  spr(128+ani,120,4)
  
end


